# Copy this file to .env and fill in the values before running docker compose

# SQL Server SA password (used by the sqlserver container and the auth-service connection string)
SA_PASSWORD=YourStrong!Passw0rd

# Shared JWT secret (same key used by both services)
JWT_KEY=YourSuperSecretKeyAtLeast32CharactersLong!

# Google OAuth credentials (Auth Service only)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# WeatherAPI.com key (Weather.Current only)
WEATHER_API_KEY=your-weatherapi-key

# Azure Application Insights (optional â€“ leave empty to disable)
APPINSIGHTS_CONNECTION_STRING=
